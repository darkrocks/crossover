.progress-bar
  height: 41px;
  width: 60px;
  margin-bottom: 2px;
  margin-top: 2px;
  border-color: #c5c5c5;
  border-style: solid;
  box-sizing: border-box;
  border-width: 2px;

  .done
  .complement
    width: 100%;
    height 100%
    float: left;
    box-sizing: border-box;

  .done
    width: 60%
    background-color cross-blue
  .complement
    width: 40%
    background-color cross-grey

  .done.success
    background-color cross-green
  .done.fail
    background-color cross-red

